{
    "manifest_version": 3,
    "name": "CFTopHacker",
    "version": "0.1",
    "description": "Adds a 'Hacks Standings' button to Codeforces contest standings page",
    "icons": {
      "16": "icons/logo.png",
      "32": "icons/logo.png",
      "48": "icons/logo.png"
    },
    "permissions": ["activeTab", "scripting"],
    "host_permissions": [
      "https://codeforces.com/*"
    ],
    "action": {
      "browser_style": true,
      "default_icon": "icons/logo.png",
      "default_title": "HacksStandings",
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://codeforces.com/contest/*/standings*","https://codeforces.com/contest/*/ratings*"],
        "js": ["jquery/jquery.min.js", "jquery/jquery.easing.min.js","jquery/jquery.lavalamp.min.js","content.js"]
      }
    ]
  }